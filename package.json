{
  "name": "productfinder-infra",
  "version": "0.1.0",
  "bin": {
    "productfinder-infra": "bin/productfinder-infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bmx:cntest": "NODE_ENV=cntest cdk deploy --require-approval never",
    "bmx:cnprod": "NODE_ENV=cnprod cdk deploy --require-approval never",
    "deploy:cntest": "NODE_ENV=cntest cdk deploy --profile CommerceGCTest",
    "deploy:cnprod": "NODE_ENV=cnprod cdk deploy --profile CommerceGCProd",
    "format": "prettier --write '{**/*.ts, **/**/*.ts}'",
    "lint": "eslint ./**/*.ts --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "20.1.7",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "aws-cdk": "2.84.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.84.0"
  }
}
